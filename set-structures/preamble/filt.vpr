function filt$Ref(g: Set[Ref]): Set[Ref]
    requires NODES(g, p)
    ensures result subset g
    ensures forall u: Ref :: 
        { u in result }
        { TAU_FILTER(u) } 
            u in g ==> (u in result <==> FILTER(u))


function filt$Int(g: Set[Ref]): Set[Ref]
    requires NODES(g, p)
    ensures result subset domain(lift(g))
    ensures forall u: Ref :: 
        { u in result }
        { TAU_FILTER(u) } 
            u in domain(lift(g)) ==> (u in result <==> FILTER(u))